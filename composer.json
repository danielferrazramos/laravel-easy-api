{
    "name": "julianobailao/laravel-easy-api",
    "type": "library",
    "description": "Laravel Easy Api Helper.",
    "keywords": [
        "juliano bailao",
        "api",
        "easy",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/julianobailao/laravel-easy-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Juliano Bail√£o",
            "email": "julianobailao@gmail.com",
            "homepage": "http://github.com/julianobailao",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "laravel/laravel": "dev-develop",
        "orchestra/testbench": "~3.4"
    },
    "autoload": {
        "psr-4": {
            "JulianoBailao\\LaravelEasyApi\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JulianoBailao\\LaravelEasyApi\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
